<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <com.google.android.gms.ads.nativead.NativeAdView
        android:id="@+id/native_ad_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/default_outline_shape">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/background"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <com.google.android.gms.ads.nativead.MediaView
                    android:id="@+id/media_view"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/default_no_size"
                    android:layout_marginTop="@dimen/default_no_margin"
                    android:adjustViewBounds="true"
                    android:scaleType="fitXY"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/middle"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:minHeight="60dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/media_view">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="@dimen/default_no_size"
                        android:layout_height="@dimen/default_no_size"
                        android:orientation="horizontal"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent">

                        <ImageView
                            android:id="@+id/icon"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginStart="@dimen/default_no_margin"
                            android:layout_marginTop="@dimen/default_no_margin"
                            android:layout_marginEnd="@dimen/default_no_margin"
                            android:layout_marginBottom="@dimen/default_no_margin"
                            android:layout_weight="0"
                            android:importantForAccessibility="no"
                            android:scaleType="fitCenter"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintDimensionRatio="H,1:1"
                            app:layout_constraintEnd_toStartOf="@id/content"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:id="@+id/content"
                            android:layout_width="@dimen/default_no_size"
                            android:layout_height="@dimen/default_no_size"
                            android:layout_marginStart="@dimen/default_default_margin"
                            android:layout_marginLeft="@dimen/default_default_margin"
                            android:orientation="vertical"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toEndOf="@id/icon"
                            app:layout_constraintTop_toTopOf="parent">

                            <LinearLayout
                                android:id="@+id/headline"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/default_no_size"
                                android:layout_weight="0.5"
                                android:background="@android:color/transparent"
                                android:gravity="left"
                                android:orientation="horizontal"
                                app:layout_constraintBottom_toTopOf="@+id/row_two"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent">

                                <TextView
                                    android:id="@+id/primary"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_marginStart="@dimen/default_no_margin"
                                    android:layout_marginTop="@dimen/default_no_margin"
                                    android:layout_marginEnd="@dimen/default_no_margin"
                                    android:layout_marginBottom="@dimen/default_no_margin"
                                    android:background="@color/default_white"
                                    android:lines="1"
                                    android:textColor="@color/default_gray"
                                    android:textSize="@dimen/default_text_size_large"
                                    android:textStyle="bold"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toEndOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />

                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/row_two"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/default_no_size"
                                android:layout_weight="0.5"
                                android:orientation="horizontal"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toBottomOf="@id/headline">

                                <TextView
                                    android:id="@+id/ad_notification_view"
                                    android:layout_width="@dimen/default_ad_indicator_width"
                                    android:layout_height="@dimen/default_ad_indicator_height"
                                    android:layout_marginStart="@dimen/default_no_margin"
                                    android:layout_marginTop="@dimen/default_ad_indicator_top_margin"
                                    android:layout_marginEnd="@dimen/default_default_margin"
                                    android:background="@drawable/default_rounded_corners_shape"
                                    android:gravity="center"
                                    android:text="Ad"
                                    android:textColor="@color/default_ad_green"
                                    android:textSize="@dimen/default_ad_indicator_text_size"
                                    android:textStyle="bold"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"
                                    tools:ignore="HardcodedText" />

                                <RatingBar
                                    android:id="@+id/rating_bar"
                                    style="?android:attr/ratingBarStyleSmall"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/default_no_margin"
                                    android:layout_marginTop="@dimen/default_no_margin"
                                    android:layout_marginEnd="@dimen/default_no_margin"
                                    android:layout_marginBottom="@dimen/default_no_margin"
                                    android:background="@android:color/transparent"
                                    android:lines="1"
                                    android:numStars="5"
                                    android:stepSize="0.1"
                                    android:textColor="@color/default_gray"
                                    android:textSize="@dimen/default_text_size_small"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toEndOf="@id/ad_notification_view"
                                    app:layout_constraintTop_toTopOf="parent">

                                </RatingBar>

                                <TextView
                                    android:id="@+id/secondary"
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:layout_marginStart="@dimen/default_no_margin"
                                    android:layout_marginTop="@dimen/default_no_margin"
                                    android:layout_marginEnd="@dimen/default_no_margin"
                                    android:layout_marginBottom="@dimen/default_no_margin"
                                    android:background="@color/default_white"
                                    android:gravity="top"
                                    android:lines="1"
                                    android:textColor="@color/default_gray"
                                    android:textSize="@dimen/default_text_size_small"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintStart_toEndOf="@id/ad_notification_view"
                                    app:layout_constraintTop_toTopOf="parent" />

                            </LinearLayout>

                        </androidx.constraintlayout.widget.ConstraintLayout>

                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:id="@+id/body"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="8dp"
                    android:padding="@dimen/default_no_margin"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/middle" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/cta"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_marginLeft="@dimen/default_no_margin"
                    android:layout_marginTop="8dp"
                    android:layout_marginRight="@dimen/default_no_margin"
                    android:background="@color/default_blue"
                    android:gravity="center"
                    android:lines="1"
                    android:textColor="@color/default_white"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHeight_min="35dp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/body" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.google.android.gms.ads.nativead.NativeAdView>

</merge>